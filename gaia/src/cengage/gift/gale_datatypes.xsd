	<xs:annotation id="datatypes-schema-purpose">
		<xs:documentation>This module defines a base set of data types to be used by Gale schemas. These include lexical or canonical representations such as date formats or identifiers such as MCode values and Ztags. The data types defined will not allow for any extension or restriction.</xs:documentation>
	</xs:annotation>
	<!--
	======================================================================
                           CHANGE HISTORY
	======================================================================
	3.0		12/04/2013		js
	1. Removed the schema owner part.
	2. Removed the unused models fixed-date-value, year-value, ISSN-value, Ingenta-mcode-value, article-ztag-value and search-date-type.
	3. Created new complex type model "common-mixed-type-model".
	4. Added the element "shared:media" to "common-mixed-type-model" [GPMS-170].
	======================================================================
-->
<xs:import namespace="http://www.gale.com/goldschema/pres" schemaLocation="presentation.xsd"/>
<xs:import namespace="http://www.gale.com/goldschema/shared" schemaLocation="shared.xsd"/>
	<xs:simpleType name="feed-status-values">
		<xs:restriction base="xs:normalizedString">
			<xs:enumeration value="Delete"/>
			<xs:enumeration value="New"/>
			<xs:enumeration value="New-Replace"/>
			<xs:enumeration value="Replace"/>
			<xs:enumeration value="ReIndex"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="delete-document-type-values">
		<xs:restriction base="xs:normalizedString">
			<xs:enumeration value="Document"/>
			<xs:enumeration value="Feed"/>
			<xs:enumeration value="MCode"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="feed-types">
		<xs:annotation>
			<xs:documentation>Enumerated values for feed type identifiers:
PER – Periodical
FREF – Fielded Reference
MMD – Multimedia Metadata
NREF – Narrative Reference
PSM – Primary Source Media
VRL – Virtual Reference Library
ATLAS - ATLAS database
PUBMETA - merged PARIS and SPUD metadata
TEXTBK - Textbook 
DICT - Dictionary
NOINDEX - Content Not Indexed
VRL_ZH - Chinese VRL</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="PER"/>
			<xs:enumeration value="FREF"/>
			<xs:enumeration value="MMD"/>
			<xs:enumeration value="NREF"/>
			<xs:enumeration value="PSM"/>
			<xs:enumeration value="VRL"/>
			<xs:enumeration value="PUBMETA"/>
			<xs:enumeration value="TEXTBK"/>
			<xs:enumeration value="DICT"/>
			<xs:enumeration value="NOINDEX"/>
			<xs:enumeration value="VRL_ZH"/>
		</xs:restriction>
	</xs:simpleType>
