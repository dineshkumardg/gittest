	<xs:simpleType name="all-mcode-values">
		<xs:annotation>
			<xs:documentation>Data type for all Gale Mcode values</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN"/>
	</xs:simpleType>
	<xs:simpleType name="Gale-mcode-value" final="#all">
		<xs:annotation>
			<xs:documentation>Data type for Gale Mcode values other than Ingenta Mcodes</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:pattern value="[A-Z0-689][A-Z0-9]{3}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="non-empty-type">
		<xs:restriction base="xs:normalizedString">
			<xs:pattern value="\s*\S[\s\S]*"/>
		</xs:restriction>
	</xs:simpleType>
